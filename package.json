{
  "name": "nullshield",
  "version": "0.4.0",
  "description": "A package containing Option and Result types",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "nyc mocha",
    "doc-build": "typedoc --module commonjs --target ES5 --out doc src/",
    "prepublishOnly": "tsc",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iwburns/nullshield.git"
  },
  "author": "Ian Burns <iwburns8@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.2.1",
    "source-map-support": "^0.5.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.3.0",
    "typedoc": "^0.9.0"
  },
  "bugs": {
    "url": "https://github.com/iwburns/nullshield/issues"
  },
  "homepage": "https://github.com/iwburns/nullshield#readme"
}
